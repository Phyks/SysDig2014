bset_brancher(v, s[0-2], S[0-7]) = (S[0-7]) where
  if s0 = 0 then
    if s1 = 0 then
      if s2 = 0 then		# 000
        S0 = v
      else 			# 001
        S1 = v
      end if
    else
      if s2 = 0 then		# 010
        S2 = v
      else 			# 011
        S3 = v
      end if
    end if
  else
    if s1 = 0 then
      if s2 = 0 then		# 100
        S4 = v
      else 			# 101
        S5 = v
      end if
    else
      if s2 = 0 then		# 110
        S6 = v
      else 			# 111
        S7 = v
      end if
    end if
  end if
end where

bset(s, regs[16-31]:[word], S:[word], PC:[word+word], STACK:[word+word], SP:[word+word]) = (regs[16-31]:[word], S:[word], PC:[word+word], STACK:[word+word], SP:[word+word]) where 
  (S[0-7]) = bset_brancher(0, s, S[0..7]);
  
  S = setStatus(S[0-7], S)
end where

bclr(s, regs[16-31]:[word], S:[word], PC:[word+word], STACK:[word+word], SP:[word+word]) = (regs[16-31]:[word], S:[word], PC:[word+word], STACK:[word+word], SP:[word+word]) where 
