lsl(d:[reg_addr], regs[16-31]:[word], S:[word], PC:[word+word], STACK:[word+word], SP:[word+word]) = (regs[16-31]:[word], S:[word], PC:[word+word], STACK:[word+word], SP:[word+word]) where
  (regs[16-31]) = add(d, d, regs[16-31], S)

end where

lsr(d:[reg_addr], regs[16-31]:[word], S:[word], PC:[word+word], STACK:[word+word], SP:[word+word]) = (regs[16-31]:[word], S:[word], PC:[word+word], STACK:[word+word], SP:[word+word]) where
  w1 = pop_register(d, regs[16-31]);

  a = (w1[wordm1]);
  w = 0. w1[0..(wordm2)];


  S0 = S[0]; #I
  S1 = S[1]; #T
  S2 = S[2]; #H
  S3 = cS(N, V); #S
  S4 = not a; #V
  S5 = 0; #N
  S6 = cZ(w); #Z
  S7 = a; #C

  S = setStatus(S[0-7]);


  (regs[16-31]) = push_register(d, w, regs[16-31])
end where

rol(d:[reg_addr], regs[16-31]:[word], S:[word], PC:[word+word], STACK:[word+word], SP:[word+word]) = (regs[16-31]:[word], S:[word], PC:[word+word], STACK:[word+word], SP:[word+word]) where
  w1 = pop_register(d, regs[16-31]);

  c = S[7];
  w = w1[1..].c;

  S0 = S[0]; #I
  S1 = S[1]; #T
  S2 = w1[4]; #H
  S3 = cS(N,V); #S
  S4 = w[0] xor w1[0]; #V
  S5 = w[0]; #N
  S6 = cZ(w); #Z
  S7 = w1[0]; #C

  S = setStatus(S[0-7]);

  (regs[16-31]) = push_register(d, w, regs[16-31])
end where

